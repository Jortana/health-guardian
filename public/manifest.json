{
  "name": "Health Guardian",
  "description": "随时掌控自己的健康状态",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "runtime", "storage"],
  "action": {
    "default_popup": "./popup/index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
